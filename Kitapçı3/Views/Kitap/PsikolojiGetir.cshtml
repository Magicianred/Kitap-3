@model Kitapçı3.Models.Psikoloji
@{
    ViewData["Title"] = "PsikolojiGetir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<img src="https://pavaedu.com/wp-content/uploads/2019/01/padova-universitesi-psikoloji-bolumu.png" style="height:500px; width:1000px; margin-left:70px;" />

<br />
<br />
@using (Html.BeginForm("PsikolojiGetir", "Psikoloji", FormMethod.Post))
{
    <br />
    <b>Kitap Adı</b>
    @Html.TextBoxFor(x => x.Kitapad, new { @class = "form-control" })
    <br />
    <b> Yazar</b>
    @Html.TextBoxFor(x => x.Yazar, new { @class = "form-control" })
    <br />
    <b> Yayınevi</b>
    @Html.TextBoxFor(x => x.Yayınevi, new { @class = "form-control" })
    <br />
    <b>Fiyat</b>
    @Html.TextBoxFor(x => x.Fiyat, new { @class = "form-control" })
}
<br />
<a href="#" class="btn btn-info"> Kitabı Satın Al</a>
<div style="margin-top:80px;">

